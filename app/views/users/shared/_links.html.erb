<%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>

      <br /><%= link_to "<button>Sign in with #{OmniAuth::Utils.camelize(provider)}</button>".html_safe, omniauth_authorize_path(resource_name, provider), :class=>"btn-btn" %><br />
      <!--https://scotch.io/tutorials/integrating-social-login-in-a-ruby-on-rails-application-->
       <!--%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider) %>-->
     
    <% end -%>       
<% end -%>
<!--MAYBE TAKE OUT LOG IN AND SIGN UP, ALREADY DISPLAYED IN LEADER..?
